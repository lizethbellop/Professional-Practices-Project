<AnchorPane id="AnchorPane" prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="practicasprofesionalespf.controller.student.FXMLPendingDeliveriesController">
   <children>
      <ImageView fitHeight="150.0" fitWidth="102.0" layoutX="34.0" layoutY="29.0" pickOnBounds="true" preserveRatio="true">
         <image><Image url="@../../images/uv_logo.png" /></image>
      </ImageView>
      <Pane layoutX="730.0" prefHeight="56.0" prefWidth="294.0" style="-fx-background-color: #015aaa;">
         <children>
            <Label layoutX="12.0" layoutY="10.0" text="Universidad Veracruzana" textFill="WHITE">
               <font><Font name="Avenir Medium" size="24.0" /></font>
            </Label>
         </children>
      </Pane>
      <Label layoutX="185.0" layoutY="118.0" text="Mis Entregas Pendientes">
         <font><Font name="Avenir Medium" size="36.0" /></font>
      </Label>
      <TableView fx:id="tvPendingDeliveries" layoutX="100.0" layoutY="200.0" prefHeight="450.0" prefWidth="824.0">
        <columns>
          <TableColumn fx:id="tcName" prefWidth="320.0" text="Nombre de la Entrega" />
          <TableColumn fx:id="tcStartDate" prefWidth="180.0" text="Fecha de Inicio" />
          <TableColumn fx:id="tcEndDate" prefWidth="180.0" text="Fecha de Cierre" />
