      </TableView>
      <Button fx:id="btnContinue" layoutX="844.0" layoutY="680.0" mnemonicParsing="false" onAction="#btnContinueClicked" prefHeight="40.0" prefWidth="120.0" style="-fx-background-color: #01ab4b;" text="Continuar" textFill="WHITE">
         <font><Font name="Avenir Medium" size="18.0" /></font>
      </Button>
      <Button layoutX="714.0" layoutY="680.0" mnemonicParsing="false" onAction="#btnExitClicked" prefHeight="40.0" prefWidth="120.0" style="-fx-background-color: #01ab4b;" text="Salir" textFill="WHITE">
         <font><Font name="Avenir Medium" size="18.0" /></font>
      </Button>
   </children>
</AnchorPane>
